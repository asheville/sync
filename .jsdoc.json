{
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/minami"
  },
  "source": {
    "excludePattern": "(node_modules/|docs)",
    "include": [
      "app.js",
      "assertions",
      "controllers",
      "factories",
      "globals.js",
      "lib",
      "models",
      "README.md",
      "routers",
      "server.js",
      "socketEvents",
      "tests"
    ],
    "includePattern": ".js$"
  }
}